{
  "manifest_version": 3,
  "name": "WDCS-Keka-TimeSense",
  "version": "1.0",
  "description": "Summarizes attendance hours from Keka",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://*.keka.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}